<%-participant_headers = []
if (@participant_registrations.first != nil) && (!@participant_structure.blank?)
    @participant_structure.each do |element|
    	participant_headers.push(element['caption'])
    end
end-%>
<%=CSV.generate_line(participant_headers).strip %>

<%- @participant_registrations.each do |registration|
  participant_row =[]
  @participant_structure.each do |element|
    participant_row.push(registration["attributes"][element["name"]])
  end-%>
  <%=CSV.generate_line(participant_row).strip %>
<% end %>

